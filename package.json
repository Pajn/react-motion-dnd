{
  "name": "react-motion-dnd",
  "version": "0.1.2",
  "description": "Drag and drop library for react with beautiful animations",
  "repository": "Pajn/react-motion-dnd",
  "homepage": "https://pajn.github.io/react-motion-dnd/",
  "author": "Rasmus Eneman <rasmus@eneman.eu>",
  "main": "lib/index.js",
  "files": [
    "lib",
    "!/**/__tests__"
  ],
  "scripts": {
    "start": "snowpack dev",
    "build": "tsc --build tsconfig.build.json",
    "build-docs": "snowpack build",
    "test": "web-test-runner \"{examples,src}/**/*.test.tsx\"",
    "format": "prettier --write \"{examples,src}/**/*.{js,jsx,ts,tsx}\"",
    "lint": "prettier --check \"{examples,src}/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "array-move": "^3.0.1",
    "framer-motion": "^2.9.4",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@snowpack/plugin-react-refresh": "^2.3.7",
    "@snowpack/plugin-typescript": "^1.1.0",
    "@snowpack/web-test-runner-plugin": "^0.1.4",
    "@testing-library/react": "^11.0.0",
    "@types/chai": "^4.2.13",
    "@types/chai-dom": "^0.0.10",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/snowpack-env": "^2.3.2",
    "@web/test-runner": "^0.9.7",
    "chai": "^4.2.0",
    "chai-dom": "^1.8.2",
    "eslint-config-react-app": "^6.0.0",
    "focus-visible": "^5.2.0",
    "history": "5",
    "prettier": "^2.0.5",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0",
    "snowpack": "^2.17.1",
    "typescript": "^4.0.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all"
  }
}
